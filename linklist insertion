        "class BookNode:\n",
        "    def __init__(self, book):\n",
        "        self.book = book\n",
        "        self.next = None\n",
        "\n",
        "class LinkedList:\n",
        "    def __init__(self):\n",
        "        self.head = None\n",
        "\n",
        "    def insert(self, book):\n",
        "        node = BookNode(book)\n",
        "        node.next = self.head\n",
        "        self.head = node\n"
      ],
